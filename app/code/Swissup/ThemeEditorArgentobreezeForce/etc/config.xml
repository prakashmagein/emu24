                <buttonTocart_color>#fff</buttonTocart_color>
                <buttonTocartHover_background>#ede734</buttonTocartHover_background>
                <buttonTocartHover_borderColor>#ede734</buttonTocartHover_borderColor>
                <buttonTocartHover_color>#202633</buttonTocartHover_color>
            </buttons>
            <fonts>
                <forceTitle_fontFamily>'Barlow Semi Condensed', sans-serif</forceTitle_fontFamily>
                <forceTitle_fontWeight>600</forceTitle_fontWeight>
                <forceTitle_color>#2b3945</forceTitle_color>
