                <blockTitle_fontWeight>600</blockTitle_fontWeight>
                <blockTitle_color>#2b3945</blockTitle_color>
                <finalPrice_color>#f76801</finalPrice_color>
            </fonts>
            <!--
            <category>
                <max_width>1200</max_width>
                <grid>
                    <columns_count>{"640":{"columns":"3","spacing":"-1"},"1024":{"columns":"4","spacing":"-1"}}</columns_count>
                </grid>
            </category>
            <product>
                <max_width>1200</max_width>
                <media_width>50</media_width>
                <infomain_width>47</infomain_width>
            </product>
            -->
            <css_selector>
<!--            Swissup_ThemeEditorArgentobreezeForce::system/layout.xml-->
                <body>body</body>
                <page>.page-wrapper</page>
                <content-container><![CDATA[.navigation, .breadcrumbs, .catalog-category-view .page-main .breadcrumbs, .catalog-category-view .page-main .columns, .page-header .header.panel, .header.content, .footer-middle, .page-wrapper > .widget, .page-wrapper > .page-bottom, .block.category.event, .top-container, .page-main, .page-container > .content, .jumbotron > .container]]></content-container>
<!--            Swissup_ThemeEditorArgentobreezeForce::system/header.xml-->
                <page-header><![CDATA[.page-header]]></page-header>
                <page-header-panel><![CDATA[.page-header .panel.wrapper]]></page-header-panel>
                <header-search>.page-header .block-search .input-text</header-search>
                <header-search-folder>.block-search.block-swissup-ajaxsearch.inline .field.search .input-text</header-search-folder>
                <header-search-close>.page-header .block-search.inline .action.close</header-search-close>
<!--            Swissup_ThemeEditorArgentobreezeForce::system/footer.xml-->
                <footer>.page-footer</footer>
                <footer-link>.footer.content .links a, .footer.content .links a:visited, .page-footer .footer-toolbar-top a, .page-footer .footer-toolbar-top .alink</footer-link>
                <footer-link-hover>.footer.content .links a:hover, .footer.content .links a:visited:hover, .page-footer .footer-toolbar-top a:hover, .page-footer .footer-toolbar-top .alink:hover</footer-link-hover>
                <footer-title>.page-footer div[data-role="title"],.page-footer .h4, .page-footer .block .block-title strong, .page-footer .block .title strong</footer-title>
                <footer-line>#html-body .page-footer div[data-content-type="divider"] hr[data-element="line"]</footer-line>
                <footer-newsletter-button>.page-footer .block.newsletter .action.subscribe</footer-newsletter-button>
                <footer-newsletter-button-hover>.page-footer .block.newsletter .action.subscribe:hover, .page-footer .block.newsletter .action.subscribe:focus, .page-footer .block.newsletter .action.subscribe:active</footer-newsletter-button-hover>
                <footer-newsletter-button-image>.page-footer .block.newsletter .action.subscribe::before</footer-newsletter-button-image>
                <footer-social>.page-footer .social-icons .fa-stack</footer-social>
                <footer-social-bg>.page-footer .social-icons .fa-inverse</footer-social-bg>
<!--            Swissup_ThemeEditorArgentobreezeForce::system/buttons.xml-->
                <button>.action.primary</button>
                <button-hover>.action.primary:hover, .action.primary:focus, .action.primary:active</button-hover>
                <button-secondary><![CDATA[button, .abs-revert-to-action-secondary, .paypal-review .block .actions-toolbar .action.primary, .abs-discount-block .actions-toolbar .action.primary, .abs-discount-block .actions-toolbar .action.cancel, .block-compare .action.primary, .cart-summary .block .actions-toolbar > .primary .action.primary, .cart-summary .form.giftregistry .fieldset .action.primary, .paypal-review-discount .actions-toolbar .action.primary, .paypal-review-discount .actions-toolbar .action.cancel, .cart-discount .actions-toolbar .action.primary, .cart-discount .actions-toolbar .action.cancel, .product.data.items .review-add .review-form .action.submit.primary, .cms-index-index a.button:not(.primary), .cms-index-index .column.main .block .actions-toolbar .action.view]]></button-secondary>
