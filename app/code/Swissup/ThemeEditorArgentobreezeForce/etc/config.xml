<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <swissup_argentobreeze_force>
            <layout>
                <body_backgroundColor>#ffffff</body_backgroundColor>
                <body_backgroundImage></body_backgroundImage>
                <body_backgroundRepeat>no-repeat</body_backgroundRepeat>
                <body_backgroundPosition>0,0</body_backgroundPosition>
                <page_maxWidth>none</page_maxWidth>
                <contentContainer_maxWidth>1260px</contentContainer_maxWidth>
            </layout>
            <header>
                <pageHeader_backgroundColor>#fff</pageHeader_backgroundColor>
                <pageHeader_backgroundImage></pageHeader_backgroundImage>
                <pageHeader_backgroundRepeat>no-repeat</pageHeader_backgroundRepeat>
