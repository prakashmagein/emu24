                <footerLine_borderWidth>1px</footerLine_borderWidth>
                <footerNewsletterButton_backgroundColor>#ede734</footerNewsletterButton_backgroundColor>
                <footerNewsletterButtonHover_backgroundColor>#dbd413</footerNewsletterButtonHover_backgroundColor>
                <footerSocial_color>#202633</footerSocial_color>
                <footerSocialBg_color>#797c84</footerSocialBg_color>
            </footer>
            <buttons>
                <button_background>#f76801</button_background>
                <button_borderColor>#f76801</button_borderColor>
                <button_color>#fff</button_color>
                <buttonHover_background>#202633</buttonHover_background>
                <buttonHover_borderColor>#202633</buttonHover_borderColor>
                <buttonHover_color>#fff</buttonHover_color>
                <buttonSecondary_background>#fff</buttonSecondary_background>
