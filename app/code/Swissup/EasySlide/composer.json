{
    "name": "swissup/module-easy-slide",
    "description": "Create and manage Magento sliders",
    "keywords": [
        "easyslide",
        "slider"
    ],
    "type": "magento2-module",
    "version": "1.8.14",
    "license": "OSL-3.0",
    "require": {
        "magento/framework": ">=102.0.2",
        "magento/module-backend": ">=101.0",
        "magento/module-cms": ">=103.0",
        "magento/module-catalog": ">=103.0",
        "magento/module-media-storage": ">=100.3",
        "magento/module-store": ">=101.0",
        "swissup/module-core": "^1.12.22",
        "swissup/module-codemirror": "^1.1.15",
        "swissup/module-image": "^1.0.0",
        "swissup/module-swiper": "^1.0.0",
        "psr/log": "*"
    },
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "Swissup\\EasySlide\\": ""
        }
