{
    "name": "swissup/module-askit",
    "description": "Product, Category, Cms Page questions and comments",
    "keywords": [
        "product questions",
        "page comments",
        "questions",
        "answers",
        "comments",
        "faq"
    ],
    "type": "magento2-module",
    "version": "1.14.11",
    "license": "OSL-3.0",
    "require": {
        "swissup/module-core": "^1.12.22",
        "magento/framework": ">=102.0",
        "magento/module-customer": ">=102.0",
        "magento/module-catalog": ">=103.0",
        "magento/module-store": ">=101.0",
        "magento/module-url-rewrite": ">=101.1",
        "magento/module-ui": ">=101.1",
        "magento/module-user": ">=101.1",
        "magento/module-graph-ql": ">=100.3",
        "magento/module-backend": ">=101.0",
        "magento/module-cms": ">=103.0",
        "magento/module-widget": ">=101.1",
        "magento/module-captcha": ">=100.3",
        "magento/module-contact": ">=100.3",
        "magento/module-directory": ">=100.3",
        "magento/module-theme": ">=101.0",
        "psr/log": "*"
    },
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "Swissup\\Askit\\": ""
        }
    },
    "extra": {
        "swissup": {
            "links": {
                "docs"     : "http://docs.swissuplabs.com/m2/extensions/askit/",
                "changelog": "http://docs.swissuplabs.com/m2/extensions/askit/changelog/",
                "store"    : "https://swissuplabs.com/product-question-extension-for-magento-2.html"
            }
        },
        "marketplace": {
            "gallery": [
                "https://docs.swissuplabs.com/images/marketplace/module-askit.png"
            ],
            "links": {
                "docs": "https://docs.swissuplabs.com/m2/extensions/askit/",
                "changelog": "https://docs.swissuplabs.com/m2/extensions/askit/changelog/"
            }
        }
    }
}
