<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
  <table name="swissup_seourls_attribute_label" resource="default" engine="innodb" comment="Swissup SEO URLs Attribute Label">
    <column xsi:type="int" name="attribute_label_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Attribute Label Id"/>
    <column xsi:type="smallint" name="attribute_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Attribute Id"/>
    <column xsi:type="smallint" name="store_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Store Id"/>
    <column xsi:type="varchar" name="value" nullable="true" length="255" comment="Value"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="attribute_label_id"/>
    </constraint>
    <constraint xsi:type="foreign" referenceId="SWISSUP_SEOURLS_ATTR_LBL_ATTR_ID_EAV_ATTR_ATTR_ID" table="swissup_seourls_attribute_label" column="attribute_id" referenceTable="eav_attribute" referenceColumn="attribute_id" onDelete="CASCADE"/>
    <constraint xsi:type="foreign" referenceId="SWISSUP_SEOURLS_ATTRIBUTE_LABEL_STORE_ID_STORE_STORE_ID" table="swissup_seourls_attribute_label" column="store_id" referenceTable="store" referenceColumn="store_id" onDelete="CASCADE"/>
    <index referenceId="SWISSUP_SEOURLS_ATTRIBUTE_LABEL_ATTRIBUTE_ID_STORE_ID" indexType="btree">
      <column name="attribute_id"/>
      <column name="store_id"/>
    </index>
  </table>
  <table name="swissup_seourls_attribute_value" resource="default" engine="innodb" comment="Swissup SEO URLs Attribute Values">
    <column xsi:type="bigint" name="attribute_value_id" padding="20" unsigned="true" nullable="false" identity="true" comment="Attribute Vaule Id"/>
    <column xsi:type="smallint" name="attribute_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Attribute Id"/>
    <column xsi:type="smallint" name="store_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Store Id"/>
    <column xsi:type="varchar" name="original_value" nullable="true" length="255" comment="Original Value"/>
    <column xsi:type="varchar" name="url_value" nullable="true" length="255" comment="In-URL Value"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="attribute_value_id"/>
    </constraint>
    <constraint xsi:type="foreign" referenceId="SWISSUP_SEOURLS_ATTR_VAL_ATTR_ID_EAV_ATTR_ATTR_ID" table="swissup_seourls_attribute_value" column="attribute_id" referenceTable="eav_attribute" referenceColumn="attribute_id" onDelete="CASCADE"/>
    <constraint xsi:type="foreign" referenceId="SWISSUP_SEOURLS_ATTRIBUTE_VALUE_STORE_ID_STORE_STORE_ID" table="swissup_seourls_attribute_value" column="store_id" referenceTable="store" referenceColumn="store_id" onDelete="CASCADE"/>
    <index referenceId="SWISSUP_SEOURLS_ATTRIBUTE_VALUE_ATTRIBUTE_ID_STORE_ID" indexType="btree">
      <column name="attribute_id"/>
      <column name="store_id"/>
    </index>
  </table>
  <table name="swissup_seourls_category_label" resource="default" engine="innodb" comment="Swissup SEO URLs Category Label">
    <column xsi:type="int" name="category_label_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Category Label Id"/>
    <column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="false" default="0" comment="Entity Id"/>
    <column xsi:type="smallint" name="store_id" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Store Id"/>
    <column xsi:type="varchar" name="value" nullable="true" length="255" comment="Value"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="category_label_id"/>
    </constraint>
    <constraint xsi:type="foreign" referenceId="SWISSUP_SEOURLS_CTGR_LBL_ENTT_ID_CAT_CTGR_ENTT_ENTT_ID" table="swissup_seourls_category_label" column="entity_id" referenceTable="catalog_category_entity" referenceColumn="entity_id" onDelete="CASCADE"/>
    <constraint xsi:type="foreign" referenceId="SWISSUP_SEOURLS_CATEGORY_LABEL_STORE_ID_STORE_STORE_ID" table="swissup_seourls_category_label" column="store_id" referenceTable="store" referenceColumn="store_id" onDelete="CASCADE"/>
    <index referenceId="SWISSUP_SEOURLS_CATEGORY_LABEL_ENTITY_ID_STORE_ID" indexType="btree">
      <column name="entity_id"/>
      <column name="store_id"/>
    </index>
  </table>
  <table name="swissup_seourls_attribute_advanced" resource="default" engine="innodb" comment="Swissup SEO URLs Attribute - advanced properties">
    <column xsi:type="smallint" name="attribute_id" padding="5" unsigned="true" nullable="false" identity="false" comment="Attribute Id"/>
    <column xsi:type="smallint" name="is_nofollow" padding="5" unsigned="true" nullable="false" identity="false" default="0" comment="Is nofollow"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="attribute_id"/>
    </constraint>
    <constraint xsi:type="foreign" referenceId="SWISSUP_SEOURLS_ATTR_ADVANCED_ATTR_ID_EAV_ATTR_ATTR_ID" table="swissup_seourls_attribute_advanced" column="attribute_id" referenceTable="eav_attribute" referenceColumn="attribute_id" onDelete="CASCADE"/>
  </table>
</schema>
