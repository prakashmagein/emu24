<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <!-- Footer -->
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\Footer\Copyright" type="Swissup\ThemeEditor\Model\Config\Backend\Footer\Copyright">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">default</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\Footer\Links" type="Swissup\ThemeEditor\Model\Config\Backend\Footer\Links">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">default</item>
            </argument>
        </arguments>
    </virtualType>

    <!-- Category -->
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Block\Adminhtml\System\Config\Form\Field\CategoryPagePreview" type="Swissup\ThemeEditor\Block\Adminhtml\System\Config\Form\Field\PagePreview">
        <arguments>
            <argument name="previewData" xsi:type="array">
                <item name="template" xsi:type="string">Swissup_ThemeEditor::config/category-page-preview.phtml</item>
                <item name="content_width" xsi:type="number">1348</item>
                <item name="description_position" xsi:type="string">main.content.before</item>
                <item name="layered_navigation_position" xsi:type="string">sidebar</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\CategoryPageLayout" type="Swissup\ThemeEditor\Model\Config\Backend\PageLayout">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_category_view</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\CategoryContentWidth" type="Swissup\ThemeEditor\Model\Config\Backend\ContentWidth">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_category_view</item>
                <item name="selector" xsi:type="string">.catalog-category-view .page-main, .catalog-category-view .breadcrumbs ul</item>
            </argument>
        </arguments>
    </virtualType>
    <type name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\Category\MoveDescription">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_category_view</item>
            </argument>
        </arguments>
    </type>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\Category\MoveLayeredNavigation" type="Swissup\ThemeEditor\Model\Config\Backend\Category\MoveLayeredNavigation">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_category_view_type_layered</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\CategoryColumnsCount" type="Swissup\ThemeEditor\Model\Config\Backend\ColumnsCountBreeze">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_category_view</item>
            </argument>
        </arguments>
    </virtualType>

    <!-- Product -->
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Block\Adminhtml\System\Config\Form\Field\ProductPagePreview" type="Swissup\ThemeEditor\Block\Adminhtml\System\Config\Form\Field\PagePreview">
        <arguments>
            <argument name="previewData" xsi:type="array">
                <item name="template" xsi:type="string">Swissup_ThemeEditor::config/product-page-preview.phtml</item>
                <item name="page_layout" xsi:type="string">_one_column</item>
                <item name="tabs_position" xsi:type="string">content</item>
                <item name="tabs_layout" xsi:type="string">expanded</item>
                <item name="image_position" xsi:type="string">vertical</item>
                <item name="tocart_position" xsi:type="string">product.info.main</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\ProductContentWidth" type="Swissup\ThemeEditor\Model\Config\Backend\ContentWidth">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_product_view</item>
                <item name="selector" xsi:type="string">.page-main, .breadcrumbs ul</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\Product\MediaBlock" type="Swissup\ThemeEditor\Model\Config\Backend\Product\MediaWidthBreeze">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_product_view</item>
                <item name="blockName" xsi:type="string">argentobreeze.product.media.width</item>
                <item name="selector" xsi:type="string">.catalog-product-view .product-info-wrapper</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\Product\MoveTabs" type="Swissup\ThemeEditor\Model\Config\Backend\Product\MoveTabs">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_product_view</item>
            </argument>
        </arguments>
    </virtualType>
    <virtualType name="Swissup\ThemeEditorArgentobreezeBusiness\Model\Config\Backend\Product\PhotoInDescription" type="Swissup\ThemeEditor\Model\Config\Backend\Product\PhotoInDescription">
        <arguments>
            <argument name="data" xsi:type="array">
                <item name="themeCode" xsi:type="string">Swissup/argentobreeze-business</item>
                <item name="handle" xsi:type="string">catalog_product_view</item>
            </argument>
        </arguments>
    </virtualType>
</config>
