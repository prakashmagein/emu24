<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Swissup_SeoCore::config">
                                <resource id="Swissup_SeoImages::config_seo_images" title="SEO Images" sortOrder="2100" />
                            </resource>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Backend::system">
                    <resource id="Swissup_SeoImages::index_index" title="SEO Images" sortOrder="10">
                        <resource id="Swissup_SeoImages::index_flush" title="Clean Cached Names" sortOrder="10" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
