<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="core" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Swissup_Gdpr/js/model/cookie-blocker" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/model/cookie-blocker</item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                    <param name="1">swissup_gdpr/cookie_consents/enabled</param>
                                </item>
                                <item name="import" xsi:type="array">
                                    <item name="manager" xsi:type="string">Swissup_Gdpr/js/model/cookie-manager</item>
                                </item>
                            </item>
                        </item>
                    </item>
                    <item name="default" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Swissup_Gdpr/js/view/cookie-bar" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/view/cookie-bar</item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                    <param name="1">swissup_gdpr/cookie_consents/enabled</param>
                                </item>
                                <item name="import" xsi:type="array">
                                    <item name="manager" xsi:type="string">Swissup_Gdpr/js/model/cookie-manager</item>
                                    <item name="blocker" xsi:type="string">Swissup_Gdpr/js/model/cookie-blocker</item>
                                </item>
                            </item>
                            <item name="Swissup_Gdpr/js/action/accept-cookie-groups" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/action/accept-cookie-groups</item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                    <param name="1">swissup_gdpr/cookie_consents/enabled</param>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="Swissup_Gdpr/js/action/register-unknown-cookie" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/action/register-unknown-cookie</item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                    <param name="1">swissup_gdpr/cookie_consents/enabled</param>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="Swissup_Gdpr/js/view/cookie-settings" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/view/cookie-settings</item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                    <param name="1">swissup_gdpr/cookie_consents/enabled</param>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onReveal" xsi:type="array">
                                        <item name="0" xsi:type="string">.cookie-settings</item>
                                    </item>
                                </item>
                            </item>
                            <item name="Swissup_Gdpr/js/view/consents" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/view/consents</item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                </item>
                            </item>
                            <item name="Swissup_Gdpr/js/view/consents-renderer" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/view/consents-renderer</item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                </item>
                                <item name="load" xsi:type="array">
                                    <item name="onRequire" xsi:type="boolean">true</item>
                                </item>
                            </item>
                            <item name="swissup-gdpr-mixins" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/breeze/mixins</item>
                                <item name="mixins" xsi:type="array">
                                    <item name="googleAnalytics" xsi:type="string">Magento_GoogleAnalytics/js/google-analytics</item>
                                    <item name="cookieNotices" xsi:type="string">cookieNotices</item>
                                </item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                    <param name="1">swissup_gdpr/cookie_consents/enabled</param>
                                </item>
                            </item>
                        </item>
                    </item>
                    <item name="customer" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Swissup_Gdpr/js/view/privacy/delete-data" xsi:type="array">
                                <item name="path" xsi:type="string">Swissup_Gdpr/js/view/privacy/delete-data</item>
                                <item name="enabled" xsi:type="helper" helper="Swissup\Breeze\Helper\Config::isAllEnabled">
                                    <param name="0">swissup_gdpr/general/enabled</param>
                                </item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
