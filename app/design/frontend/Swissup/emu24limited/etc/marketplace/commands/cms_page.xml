<?xml version="1.0"?>
<config>
    <command class="Swissup\Marketplace\Installer\Command\CmsPage">
        <data>
            <item>
                <item name="title">Argento Business Homepage</item>
                <item name="identifier">home</item>
                <item name="page_layout">cms-full-width</item>
                <item name="content" helper="Swissup\Marketplace\Installer\Helper\Renderer::render">
                    <param name="path" type="path">content/cms_page/home.html</param>
                </item>
            </item>
            <item>
                <item name="title">Argento Business Typography</item>
                <item name="identifier">typography</item>
                <item name="page_layout">cms-full-width</item>
                <item name="content" helper="Swissup\Marketplace\Installer\Helper\Renderer::render">
                    <param name="path" type="path">content/cms_page/typography.html</param>
                </item>
            </item>
            <item>
                <item name="title">Argento Business Blog</item>
                <item name="identifier">blog</item>
                <item name="page_layout">cms-full-width</item>
                <item name="content" helper="Swissup\Marketplace\Installer\Helper\Renderer::render">
                    <param name="path" type="path">content/cms_page/blog.html</param>
                </item>
            </item>
            <item>
                <item name="title">Argento Business FAQs</item>
                <item name="identifier">faq</item>
                <item name="page_layout">cms-full-width</item>
                <item name="content_heading">FAQs</item>
                <item name="content" helper="Swissup\Marketplace\Installer\Helper\Renderer::render">
                    <param name="path" type="path">content/cms_page/faq.html</param>
                </item>
            </item>
            <item>
                <item name="title">Argento Business About us</item>
                <item name="identifier">about</item>
                <item name="page_layout">cms-full-width</item>
                <item name="content" helper="Swissup\Marketplace\Installer\Helper\Renderer::render">
                    <param name="path" type="path">content/cms_page/about.html</param>
                </item>
            </item>
        </data>
    </command>
</config>
